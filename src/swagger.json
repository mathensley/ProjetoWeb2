        "/products": {
            "post": {
                "summary": "Cadastro de produto",
                "description": "Rota responsável por cadastrar os produtos",
                "tags": ["Products"],
                "security": [{"bearerAuth": []}],
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": { "type": "string" },
                                    "description": { "type": "string" },
                                    "category": {
                                        "type": "string",
                                        "enum": ["BOLO", "DOCE", "BEBIDA"]
                                    },
                                    "price": { "type": "number", "format": "float" },
                                    "promotion": { "type": "boolean" },
                                    "establishmentId": { "type": "string" },
                                    "image": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "required": ["name", "category", "price"]
                            },
                            "encoding": {
                                "image": {
                                    "contentType": "image/png, image/jpeg"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "401": {
                        "description": "Unauthorized"
                    },
                    "400": {
                        "description": "Product already exists"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/Product"
                                }
                            }
                        }
                    }
                }
            },

            "get": {
                "summary": "Busca por todos os produtos",
                "description": "Busca por todos os produtos já cadastrados",
                "tags": ["Products"],
                "security": [{"bearerAuth": []}],
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Número da página",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "size",
                        "in": "query",
                        "description": "Limite de produtos por página",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "$ref": "#/components/schemas/ProductResponse"
                                }
                            }
                        }
                    }
                }
            }
        },

